{
    "manifest_version": 3,
    "name": "Arsenal Dashboard",
    "version": "1.0",
    "description": "Get the latest matches for Arsenal.",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://v3.football.api-sports.io/*"
    ],
    "background": {
        "service_worker": "background.js"
    }
}